<?xml version="1.0" encoding="Windows-1252"?>
<xs:schema xmlns="http://tempuri.org/ilias_crs_5_0" elementFormDefault="unqualified" targetNamespace="http://tempuri.org/ilias_crs_5_0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Course">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="MetaData" />
        <!--<xs:element minOccurs="0" maxOccurs="1" ref="AdvancedMetaData" />-->
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="Admin" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Tutor" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Member" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Subscriber" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="WaitingList" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Settings" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Sort" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="ContainerSetting" />
      </xs:sequence>
      <xs:attribute name="exportVersion" type="xs:string" use="required" />
      <xs:attribute name="id" type="xs:string" />
      <xs:attribute name="showMembers">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Yes" />
            <xs:enumeration value="No" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Admin">
    <xs:complexType>
      <xs:attribute name="id" type="xs:string" use="required" />
      <xs:attribute name="notification">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Yes" />
            <xs:enumeration value="No" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="passed">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Yes" />
            <xs:enumeration value="No" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="action">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Attach" />
            <xs:enumeration value="Detach" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Tutor">
    <xs:complexType>
      <xs:attribute name="id" type="xs:string" use="required" />
      <xs:attribute name="notification" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Yes" />
            <xs:enumeration value="No" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="passed" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Yes" />
            <xs:enumeration value="No" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="action">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Attach" />
            <xs:enumeration value="Detach" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Member">
    <xs:complexType>
      <xs:attribute name="id" type="xs:string" use="required" />
      <xs:attribute name="blocked">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Yes" />
            <xs:enumeration value="No" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="passed">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Yes" />
            <xs:enumeration value="No" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="action">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Attach" />
            <xs:enumeration value="Detach" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Subscriber">
    <xs:complexType>
      <xs:attribute name="id" type="xs:string" use="required" />
      <xs:attribute name="subscriptionTime" type="xs:string" use="required" />
      <xs:attribute name="action">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Attach" />
            <xs:enumeration value="Detach" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="WaitingList">
    <xs:complexType>
      <xs:attribute name="id" type="xs:string" use="required" />
      <xs:attribute name="subscriptionTime" type="xs:string" use="required" />
      <xs:attribute name="action">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Attach" />
            <xs:enumeration value="Detach" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Settings">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Availability" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Syllabus" />
        <xs:element minOccurs="0" maxOccurs="1" ref="ImportantInformation" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Contact" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Registration" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Archive" />
        <xs:element minOccurs="0" maxOccurs="1" ref="ViewMode" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Availability">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="NotAvailable" />
        <xs:element ref="Unlimited" />
        <xs:element ref="TemporarilyAvailable" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="NotAvailable">
    <xs:complexType />
  </xs:element>
  <xs:element name="Unlimited">
    <xs:complexType />
  </xs:element>
  <xs:element name="TemporarilyAvailable">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Start" />
        <xs:element ref="End" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Start" type="xs:string" />
  <xs:element name="End" type="xs:string" />
  <xs:element name="Syllabus" type="xs:string" />
  <xs:element name="ImportantInformation" type="xs:string" />
  <xs:element name="Contact">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Name" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Responsibility" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Phone" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Email" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Consultation" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Name" type="xs:string" />
  <xs:element name="Responsibility" type="xs:string" />
  <xs:element name="Phone" type="xs:string" />
  <xs:element name="Email" type="xs:string" />
  <xs:element name="Consultation" type="xs:string" />
  <xs:element name="Registration">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element ref="Disabled" />
          <xs:element ref="Unlimited" />
          <xs:element ref="TemporarilyAvailable" />
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="1" ref="Password" />
      </xs:sequence>
      <xs:attribute name="registrationType">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Confirmation" />
            <xs:enumeration value="Direct" />
            <xs:enumeration value="Password" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="maxMembers" type="xs:string" />
      <xs:attribute name="notification">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Yes" />
            <xs:enumeration value="No" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="waitingList">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Yes" />
            <xs:enumeration value="No" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Password" type="xs:string" />
  <xs:element name="Disabled">
    <xs:complexType />
  </xs:element>
  <xs:element name="Sort">
    <xs:complexType>
      <xs:attribute name="type" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Manual" />
            <xs:enumeration value="Title" />
            <xs:enumeration value="Activation" />
            <xs:enumeration value="Creation" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="direction">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="ASC" />
            <xs:enumeration value="DESC" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="position">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="TOP" />
            <xs:enumeration value="Bottom" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="order">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Title" />
            <xs:enumeration value="Activation" />
            <xs:enumeration value="Creation" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Archive">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Start" />
        <xs:element ref="End" />
      </xs:sequence>
      <xs:attribute name="Access" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Disabled" />
            <xs:enumeration value="Read" />
            <xs:enumeration value="Download" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="ContainerSetting">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute name="id" type="xs:string" use="required" />
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="ViewMode" type="xs:string" />

  <xs:element name="ContentObject">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="MetaData" />
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="StructureObject" />
          <xs:element ref="PageObject" />
          <xs:element ref="MediaObject" />
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="1" ref="Properties" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Glossary" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Bibliography" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Layout" />
      </xs:sequence>
      <xs:attribute name="Type" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="LearningModule" />
            <xs:enumeration value="LibObject" />
            <xs:enumeration value="Glossary" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="StructureObject">
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element ref="MetaData" />
          <xs:choice minOccurs="1" maxOccurs="unbounded">
            <xs:element ref="StructureObject" />
            <xs:element ref="PageObject" />
          </xs:choice>
        </xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Layout" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PageObject">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:sequence>
            <xs:element ref="MetaData" />
            <xs:element minOccurs="0" maxOccurs="unbounded" ref="PageContent" />
          </xs:sequence>
          <xs:element ref="PageAlias" />
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="1" ref="Layout" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="PageContent">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:element ref="Paragraph" />
          <xs:element ref="MediaObject" />
          <xs:element ref="Table" />
          <xs:element ref="List" />
          <xs:element ref="FileList" />
          <xs:element ref="Question" />
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="BibItemIdentifier" />
      </xs:sequence>
      <xs:attribute name="Enabled">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="True" />
            <xs:enumeration value="False" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="BibItemIdentifier">
    <xs:complexType>
      <xs:attribute name="Catalog" type="xs:string" />
      <xs:attribute name="Entry" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="PageAlias">
    <xs:complexType>
      <xs:attribute name="OriginId" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="PageTurn">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="BibItemIdentifier" />
      </xs:sequence>
      <xs:attribute name="NumberingType" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Roman" />
            <xs:enumeration value="Arabic" />
            <xs:enumeration value="Alphanumeric" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="Number" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="MediaObject">
    <xs:complexType>
      <xs:choice>
        <xs:sequence>
          <xs:element ref="MetaData" />
          <xs:element minOccurs="1" maxOccurs="unbounded" ref="MediaItem" />
        </xs:sequence>
        <xs:sequence>
          <xs:element ref="MediaAlias" />
          <xs:element minOccurs="1" maxOccurs="unbounded" ref="MediaAliasItem" />
        </xs:sequence>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="MediaAlias">
    <xs:complexType>
      <xs:attribute name="OriginId" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="MediaAliasItem">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="1" ref="Layout" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Caption" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Parameter" />
      </xs:sequence>
      <xs:attribute name="Purpose" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Standard" />
            <xs:enumeration value="Fullscreen" />
            <xs:enumeration value="Additional" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="MediaItem">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="Location" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Format" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Layout" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Caption" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Parameter" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="MapArea" />
      </xs:sequence>
      <xs:attribute name="Purpose" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Standard" />
            <xs:enumeration value="Fullscreen" />
            <xs:enumeration value="Additional" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="MapArea">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="ExtLink" />
        <xs:element ref="IntLink" />
      </xs:choice>
      <xs:attribute name="Shape" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Rect" />
            <xs:enumeration value="Circle" />
            <xs:enumeration value="Poly" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="Coords" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Properties">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Property" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Property">
    <xs:complexType>
      <xs:attribute name="Name" type="xs:string" use="required" />
      <xs:attribute name="Value" type="xs:string" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="MetaData">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="General" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Lifecycle" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Meta-Metadata" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Technical" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Educational" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Rights" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Relation" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Annotation" />
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Classification" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="General">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="Identifier" />
        <xs:element ref="Title" />
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="Language" />
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="Description" />
        <xs:element minOccurs="1" maxOccurs="unbounded" ref="Keyword" />
        <xs:element minOccurs="0" maxOccurs="1" ref="Coverage" />
      </xs:sequence>
      <xs:attribute name="Structure" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Atomic" />
            <xs:enumeration value="Collection" />
            <xs:enumeration value="Networked" />
            <xs:enumeration value="Hierarchical" />
            <xs:enumeration value="Linear" />
          </xs:restriction>
        </xs:simpleType>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="Identifier">
    <xs:complexType>
      <xs:attribute name="Catalog" type="xs:string" />
      <xs:attribute name="Entry" type="xs:ID" use="required" />
    </xs:complexType>
  </xs:element>
  <xs:element name="Title">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="br" />
        <xs:element ref="Emph" />
        <xs:element ref="Strong" />
        <xs:element ref="Quotation" />
        <xs:element ref="Code" />
      </xs:choice>
      <xs:attribute name="Language" use="required">
        <xs:simpleType>
          <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="none" />
            <xs:enumeration value="aa" />
            <xs:enumeration value="ab" />
            <xs:enumeration value="af" />
