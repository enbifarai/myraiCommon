@using myRaiCommonModel
@model AnagraficaContenzioso

@{
    var listAction = new List<myRaiHelper.ActionTemplate>();
}

@using (Html.BeginBlock(myRaiHelper.BlockType.ContentTable, "Chiuse", listAction))
{
    @Html.Partial( "~/Views/Anagrafica/subpartial/_ElencoContenzioso.cshtml" , Model.Cause.Where( w => w.Stato.Equals( "Estinta" )
                                                                                                || w.Stato.ToUpper( ).Contains( "CHIUSO" )
                                                                                                || w.Stato.ToUpper( ).Contains( "CHIUSA" ) ).ToList( ) )
}