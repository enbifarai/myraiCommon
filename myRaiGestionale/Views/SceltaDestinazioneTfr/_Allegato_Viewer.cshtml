@using myRaiCommonModel

@model SceltaDestinazioneTfrAllegatoViewModel

@{
    string title = "Documento compilato";
}

@using (Html.BeginModal(title))
{
    <div class="row">
        <div class="col-sm-12">
            @using (Html.BeginPanel(myRaiHelper.PanelType.Panel, Model.Nome, false))
            {
                <div class="row">
                    <div class="col-xs-12">
                        <div class="col-xs-12" style="background-color:gray; overflow-y:auto;">
                            <iframe style="width:100%; height:76vh!important;" src="@Url.Action("GetAllegato","SceltaDestinazioneTfr")?idAllegato=@Model.Id"></iframe>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
}