@model myRaiData.Incentivi.XR_INC_DIPENDENTI

<form id="form-modificaVerbaleFirma" novalidate="novalidate" method="post">
    @Html.HiddenFor(x => x.ID_DIPENDENTE)

    <div class="form-group text-left">
        <div class="row push-5">
            <div class="col-sm-12 text-center">
                <br />
                <i class="icons icon-pencil text-primary" style="font-size:70px;"></i>
                <br />
            </div>
        </div>
        <div class="row push-5">
            <div class="col-sm-12">
                <label>L'avanzamento della pratica conferma l'avvenuta firma del verbale da parte del dipendente.</label>
            </div>
        </div>
    </div>
    <hr />
    <div class="row push-5">
        <div class="col-sm-12">
            <button type="submit" class="btn btn-primary" onclick="SubmitModificaVerbaleFirma(@Model.ID_DIPENDENTE)">Prosegui</button>
        </div>
    </div>
</form>