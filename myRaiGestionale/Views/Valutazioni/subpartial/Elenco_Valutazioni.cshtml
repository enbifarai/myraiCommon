@model List<myRaiCommonModel.Gestionale.SheetContainer>

<div id="panelElencoValutazioni">
    @foreach (var sheet in Model.OrderBy(x=>x.Sheet.XR_VAL_CAMPAIGN.DTA_START).ThenBy(x=>x.Sheet.VALID_DTA_INI))
    {
        string title = sheet.Sheet.XR_VAL_CAMPAIGN.NAME + " - " + sheet.Sheet.DESCRIPTION;
        @Html.RenderPanel(myRaiHelper.PanelType.PanelNoPadding, title, "subpartial/Elenco_Valutazioni_Scheda", sheet, true, 
                new List<myRaiHelper.ActionTemplate>() { new myRaiHelper.ActionTemplate() { Icon="fa fa-refresh", OnClick= "ValUpdateValutazioniSheet("+sheet.Sheet.ID_CAMPAIGN_SHEET+")" } })
    }
</div>


