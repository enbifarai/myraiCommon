@model myRaiCommonModel.SectionDayModel

<div style="width: 100%" data-async="/Home/index_section2?GappClosed=@Model.GappClosed">

    
         @if (Model.IsPreview)
         {
             if (!Model.GappClosed)
             { 
              @Html.Partial("subpartial/orario_skeleton")
             
             }
             

