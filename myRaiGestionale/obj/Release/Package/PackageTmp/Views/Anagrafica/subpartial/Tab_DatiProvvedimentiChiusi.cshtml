@using myRaiCommonModel
@model AnagraficaContenzioso

@{
    var listAction = new List<myRaiHelper.ActionTemplate>();
}

@using (Html.BeginBlock(myRaiHelper.BlockType.ContentTable, "Chiuse", listAction))
{
    @Html.Partial( "~/Views/Anagrafica/subpartial/_ElencoProvvedimenti.cshtml" , Model.Provvedimenti.Where(w => w.Stato.Equals( "DEFINITIVO" ) ).ToList())
}