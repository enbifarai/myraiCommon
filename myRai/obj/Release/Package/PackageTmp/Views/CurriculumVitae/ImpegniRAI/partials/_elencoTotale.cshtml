@model List<myRaiCommonModel.cvModels.cvModel.ImpegniRAI>

@{
    myRai.Data.CurriculumVitae.cv_ModelEntities cvEnt = new myRai.Data.CurriculumVitae.cv_ModelEntities();

    string matricola = myRaiHelper.UtenteHelper.EsponiAnagrafica()._matricola;

    
}
    
<div class="block">
        <div class="block block-themed block-transparent remove-margin-b">
              <div class=" block-header bg-primary-dark">
                <h3 class="block-title">Impegni editoriali</h3>
              </div>
              <div class="">
    <div class="form-group">
        <!-- Composizione della FORM -->
        <label>Impegni e attività editoriali Rai</label>
        @foreach (var item in Model)
        {
            <div class="row">
                <div class="col-xs-2">
                    <span class="number">@item._progDaStampare</span>
                </div>
                <div class="col-xs-10">@item._matricolaSpett&nbsp;@item._desTitoloDefinit</div>
            </div>
            
            string data_i = "";
            string data_f = "";
            
            data_i = (item._dtDataInizio != "") ? "dal " + item._dtDataInizio.ToString() : "";
            data_f = (item._dtDataFine != "") ? " al " + item._dtDataFine.ToString() : "";
            
            <span>@data_i@data_f</span>
            <span>&nbsp;@item._ruolo</span>
        }

        <!-- fine form -->
    </div> <!-- FREAK - COPIARE FINO A QUAAAAAAAAAAAA -->
              </div>
