@using myRaiData.Incentivi
@using myRaiHelper
@using myRai.Helpers;
@model XR_DEM_DOCUMENTI

<tr class="trdata visible-lg bgrow" data-idpdf="">
    <td class="text-center"  colspan="2">
        Data emissione <br />@(Model.DataInvioNotifica.GetValueOrDefault( ).ToString( "dd/MM/yyyy" ))
    </td>
    <td class="font-w600 font90">
        @(Model.Descrizione)
    </td>
    <td class="font-w600 font90" colspan="2">
        @(Model.Note)
    </td>
    <td class="text-right" style="width: 100px;">
        <a href="#" onclick="" class="btn btn-default btn-scriv text-uppercase bg-puls_dash button-viewpdf">
            VISUALIZZA
        </a>
    </td>
</tr>

<tr class="trdata hidden-lg" data-idpdf="">
    <td class="text-center" colspan="2">
        Data emissione <br />@(Model.DataInvioNotifica.GetValueOrDefault( ).ToString( "dd/MM/yyyy" ))
    </td>
    <td class="font-w600 text-info">
        @(Model.Descrizione)
    </td>
    <td class="font-w600 text-info" colspan="2">
        @(Model.Note)
    </td>
    <td class="text-right" style="width: 100px;">
        <a href="#" onclick="" class="btn btn-default btn-scriv text-uppercase bg-puls_dash button-viewpdf">
            ANTEPRIMA
        </a>
    </td>
</tr>