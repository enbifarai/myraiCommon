@using myRaiHelper;
@model List<myRaiCommonModel.Gestionale.SheetContainer>

<div id="panelElencoValutazioni">
    @foreach (var sheet in Model.Where(x=>(x.Roles!=null && x.Roles.Any(y=>y.Evaluations.Any())) 
                                        || (x.SubRoles!=null && x.SubRoles.Any(y=>y.Evaluations.Any()))))
    {
        <div id="panelElencoValutazioni@(sheet.Sheet.ID_CAMPAIGN_SHEET.ToString())">
            @Html.Partial("subpartial/Elenco_Valutazioni_Scheda", sheet)
        </div>
    }
</div>